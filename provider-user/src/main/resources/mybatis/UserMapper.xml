<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ticket.provideruser.mapper.UserMapper">

    <select id="findUser" resultType="com.ticket.provideruser.domain.User">
        select *
        from ticket.user
    </select>
    <select id="findById" resultType="com.ticket.provideruser.domain.User">
        select *
        from ticket.user
        where id = #{id}
    </select>
</mapper>
